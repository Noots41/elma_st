<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ModelDomain"
                   namespace="Models">

  <class name="OperationResult"  table="OperationResult">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="ArgumentCount" />
    <property name="Arguments" />
    <property name="Result" />
    <property name="ExetTimeMs" />

    <many-to-one name="Users" class="Users" column="UserId" lazy ="false" cascade = "save-update"/>   
    <many-to-one name="Operation" class="Operations" column="OperationId" lazy ="false" cascade = "save-update"/>
  </class>
</hibernate-mapping>